{
  "name": "susanin",
  "version": "0.0.1",
  "description": "URL shortener and redirect service",
  "main": "./.backend/index.js",
  "private": true,
  "scripts": {
    "start": "node ./.backend/index.js",
    "start:dev": "nodemon",
    "test": "mocha --reporter spec --timeout 10000 --slow 2000 ./.backend/test/",
    "test:watch": "mocha --reporter spec --timeout 10000 --slow 2000 --watch ./.backend/test/",
    "lint": "gulp lint",
    "build:clear": "gulp clear",
    "build:images": "gulp client:images && gulp client:images-resize",
    "build:icons": "gulp client:icons",
    "build:full": "npm run build:images && npm run build:icons && npm run build",
    "build": "gulp default",
    "build:server": "gulp server:js",
    "build:client": "gulp client:js && gulp client:styles",
    "build:watch": "gulp watch",
    "build:apidoc": "apidoc -i ./backend/routes -o ./docs-api",
    "prepublish": "gulp clear && gulp lint && gulp default && exit 0",
    "precommit": "gulp lint"
  },
  "author": {
    "name": "SuperPaintman",
    "email": "SuperPaintmanDeveloper@gmail.com",
    "url": "https://flatdev.ru/"
  },
  "license": "MIT",
